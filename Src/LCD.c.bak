void LCD_DetailsModeStatus(void)
{
	if(g_deSw1ShortFlg == 1)		// Change to Lower Details List Selection
	{
		if(g_deChSelStt == 0)		// if Character is  Unselected,
		{
			if(g_deDetSelStt == 0)	// if Detail(Alarm Onoff, Newwork, Simple Moving Average) is Unselected,
				g_deDetSelStt = 3;
			else
				g_deDetSelStt--;
		}
		else						// if Character is Selected
		{
			switch(g_deDetSelStt)
			{
				case 1 :						// Selected External Input Output
					g_deSetModeIoNum = 0;
					break;
				case 2 :						// Selected Network
					break;
				case 3 :						// Selected SmaSample
					break;
				default :
					break;
			}
		}
		g_deSw1ShortFlg = 0;
	}

	if(g_deSw2ShortFlg == 1)		// Change to Upper Details List Selection
	{
		if(g_deChSelStt == 0)		// if Character is  Unselected,
		{
			if(g_deDetSelStt == 3)	// if Detail(Alarm Onoff, Newwork, Simple Moving Average) is Unselected,
				g_deDetSelStt = 0;
			else
				g_deDetSelStt++;
		}
		else						// if Character is Selected
		{
			switch(g_deDetSelStt)
			{
				case 1 :						// Selected Date

					break;
				default :
					break;
			}
		}
		g_deSw2ShortFlg = 0;
	}

	if(g_deSw3ShortFlg == 1)		// Change to Details List Select Screen
	{
		if(g_deDetSelStt == 0)		// if Select 'SAVE' Details
		{
			// Save
			g_deBkgdStt = SETTING_STATE;
		}
		else						// if Select Other Details
		{
			if(g_deChSelStt < 4)
			{
				g_deChSelStt++;
			}
			else
			{
				g_deChSelStt = 0;
			}
		}
		g_deSw3ShortFlg = 0;
	}
	if(g_deSw4ShortFlg == 1)		// Change to Details List Select Screen
	{
		if(g_deChSelStt == 0)
		{
			g_deBkgdStt = SETTING_STATE;
		}
		else
		{
			g_deChSelStt = 0;
		}
		g_deSw4ShortFlg = 0;
	}
}
void LCD_DetailsNetworkStatus(void)
{
	if(g_deSw1ShortFlg == 1)		// Change to Lower Details List Selection
	{
		if(g_deChSelStt == 0)
		{
			if((g_deMainModeNetNum & 2) == 2)	// if Include Client Mode, 2 Pages
			{
				if(g_deDetSelStt == 0)
					g_deDetSelStt = 9;
				else if(g_deDetSelStt == 8)
					g_deDetSelStt = 6;
				else if(g_deDetSelStt == 2)
					g_deDetSelStt = 0;
				else
					g_deDetSelStt --;
			}
			else								// if Exclude Client Mode, 1 Page
			{
				if(g_deDetSelStt == 0)
					g_deDetSelStt = 6;
				else if(g_deDetSelStt == 2)
					g_deDetSelStt = 0;
				else
					g_deDetSelStt --;
			}
		}
		else
		{
			switch(g_deDetSelStt)
			{
				case 2 :						// Selected SensorIp
					if(g_arSetModIpNum[(g_deChSelStt-9)/4] > g_arSetAddrMin[g_deChSelStt])
						g_arSetModIpNum[(g_deChSelStt-9)/4] -= g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModIpNum[(g_deChSelStt-9)/4] = 0;
					ConvertNum2AscDec(g_arSetModIpNum[0], g_arSetModIpAsc, 3);
					ConvertNum2AscDec(g_arSetModIpNum[1], g_arSetModIpAsc+4, 3);
					ConvertNum2AscDec(g_arSetModIpNum[2], g_arSetModIpAsc+8, 3);
					ConvertNum2AscDec(g_arSetModIpNum[3], g_arSetModIpAsc+12, 3);
					break;
				case 3 :						// Selected SensorSubnet
					if(g_arSetModSubNum[(g_deChSelStt-9)/4] > g_arSetAddrMin[g_deChSelStt])
						g_arSetModSubNum[(g_deChSelStt-9)/4] -= g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModSubNum[(g_deChSelStt-9)/4] = 0;
					ConvertNum2AscDec(g_arSetModSubNum[0], g_arSetModSubAsc, 3);
					ConvertNum2AscDec(g_arSetModSubNum[1], g_arSetModSubAsc+4, 3);
					ConvertNum2AscDec(g_arSetModSubNum[2], g_arSetModSubAsc+8, 3);
					ConvertNum2AscDec(g_arSetModSubNum[3], g_arSetModSubAsc+12, 3);
					break;
				case 4 :						// Selected SensorGateway
					if(g_arSetModGwNum[(g_deChSelStt-9)/4] > g_arSetAddrMin[g_deChSelStt])
						g_arSetModGwNum[(g_deChSelStt-9)/4] -= g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModGwNum[(g_deChSelStt-9)/4] = 0;
					ConvertNum2AscDec(g_arSetModGwNum[0], g_arSetModGwAsc, 3);
					ConvertNum2AscDec(g_arSetModGwNum[1], g_arSetModGwAsc+4, 3);
					ConvertNum2AscDec(g_arSetModGwNum[2], g_arSetModGwAsc+8, 3);
					ConvertNum2AscDec(g_arSetModGwNum[3], g_arSetModGwAsc+12, 3);
					break;
				case 5 :						// Selected SensorPort
					if(g_deSetModPortNum > g_arSetPortMin[g_deChSelStt])
						g_deSetModPortNum -= g_arSetPortSum[g_deChSelStt];
					else
						g_deSetModPortNum = 0;
					ConvertNum2AscDec(g_deSetModPortNum, g_arSetModPortAsc, 5);
					break;
				case 6 :						// Selected ModMac
					if(g_arSetModMacNum[(g_deChSelStt-9)/3] < g_arSetMacMax[g_deChSelStt])
						g_arSetModMacNum[(g_deChSelStt-9)/3] += g_arSetMacSum[g_deChSelStt];
					else
						g_arSetModMacNum[(g_deChSelStt-9)/3] = 255;
					ConvertNum2AscHex(g_arSetModMacNum[0], g_arSetModMacAsc, 2);
					ConvertNum2AscHex(g_arSetModMacNum[1], g_arSetModMacAsc+3, 2);
					ConvertNum2AscHex(g_arSetModMacNum[2], g_arSetModMacAsc+6, 2);
					ConvertNum2AscHex(g_arSetModMacNum[3], g_arSetModMacAsc+9, 2);
					ConvertNum2AscHex(g_arSetModMacNum[4], g_arSetModMacAsc+12, 2);
					ConvertNum2AscHex(g_arSetModMacNum[5], g_arSetModMacAsc+15, 2);
					break;
				case 8 :						// Selected ServerIp
					if(g_arSetDestIpNum[(g_deChSelStt-9)/4] > g_arSetAddrMin[g_deChSelStt])
						g_arSetDestIpNum[(g_deChSelStt-9)/4] -= g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetDestIpNum[(g_deChSelStt-9)/4] = 0;
					ConvertNum2AscDec(g_arSetDestIpNum[0], g_arSetDestIpAsc, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[1], g_arSetDestIpAsc+4, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[2], g_arSetDestIpAsc+8, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[3], g_arSetDestIpAsc+12, 3);
					break;
				case 9 :						// Selected ServerPort
					if(g_deSetDestPortNum > g_arSetPortMin[g_deChSelStt])
						g_deSetDestPortNum -= g_arSetPortSum[g_deChSelStt];
					else
						g_deSetDestPortNum = 0;
					ConvertNum2AscDec(g_deSetDestPortNum, g_arSetDestPortAsc, 5);
					break;
				default :
					break;
			}
		}
		g_deSw1ShortFlg = 0;
	}

	if(g_deSw2ShortFlg == 1)		// Change to Upper Details List Selection
	{
		if(g_deChSelStt == 0)
		{
			if((g_deMainModeNetNum & 2) == 2)	// if Include Client Mode, 2 Pages
			{
				if(g_deDetSelStt == 9)
					g_deDetSelStt = 0;
				else if(g_deDetSelStt == 0)
					g_deDetSelStt = 2;
				else if(g_deDetSelStt == 6)
					g_deDetSelStt = 8;
				else
					g_deDetSelStt ++;
			}
			else								// if Exclude Client Mode, 1 Page
			{
				if(g_deDetSelStt == 6)
					g_deDetSelStt = 0;
				else if(g_deDetSelStt == 0)
					g_deDetSelStt = 2;
				else
					g_deDetSelStt ++;
			}
		}
		else
		{
			switch(g_deDetSelStt)
			{
				case 2 :						// Selected ModIp
					if(g_arSetModIpNum[(g_deChSelStt-9)/4] < g_arSetAddrMax[g_deChSelStt])
						g_arSetModIpNum[(g_deChSelStt-9)/4] += g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModIpNum[(g_deChSelStt-9)/4] = 255;
					ConvertNum2AscDec(g_arSetModIpNum[0], g_arSetModIpAsc, 3);
					ConvertNum2AscDec(g_arSetModIpNum[1], g_arSetModIpAsc+4, 3);
					ConvertNum2AscDec(g_arSetModIpNum[2], g_arSetModIpAsc+8, 3);
					ConvertNum2AscDec(g_arSetModIpNum[3], g_arSetModIpAsc+12, 3);
					break;
				case 3 :						// Selected ModSubnet
					if(g_arSetModSubNum[(g_deChSelStt-9)/4] < g_arSetAddrMax[g_deChSelStt])
						g_arSetModSubNum[(g_deChSelStt-9)/4] += g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModSubNum[(g_deChSelStt-9)/4] = 255;
					ConvertNum2AscDec(g_arSetModSubNum[0], g_arSetModSubAsc, 3);
					ConvertNum2AscDec(g_arSetModSubNum[1], g_arSetModSubAsc+4, 3);
					ConvertNum2AscDec(g_arSetModSubNum[2], g_arSetModSubAsc+8, 3);
					ConvertNum2AscDec(g_arSetModSubNum[3], g_arSetModSubAsc+12, 3);
					break;
				case 4 :						// Selected ModGateway
					if(g_arSetModGwNum[(g_deChSelStt-9)/4] < g_arSetAddrMax[g_deChSelStt])
						g_arSetModGwNum[(g_deChSelStt-9)/4] += g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetModGwNum[(g_deChSelStt-9)/4] = 255;
					ConvertNum2AscDec(g_arSetModGwNum[0], g_arSetModGwAsc, 3);
					ConvertNum2AscDec(g_arSetModGwNum[1], g_arSetModGwAsc+4, 3);
					ConvertNum2AscDec(g_arSetModGwNum[2], g_arSetModGwAsc+8, 3);
					ConvertNum2AscDec(g_arSetModGwNum[3], g_arSetModGwAsc+12, 3);
					break;
				case 5 :						// Selected ModPort
					if(g_deSetModPortNum < g_arSetPortMax[g_deChSelStt])
						g_deSetModPortNum += g_arSetPortSum[g_deChSelStt];
					else
						g_deSetModPortNum = 65535;
					ConvertNum2AscDec(g_deSetModPortNum, g_arSetModPortAsc, 5);
					break;
				case 6 :						// Selected ModMac
					if(g_arSetModMacNum[(g_deChSelStt-9)/3] > g_arSetMacMin[g_deChSelStt])
						g_arSetModMacNum[(g_deChSelStt-9)/3] -= g_arSetMacSum[g_deChSelStt];
					else
						g_arSetModMacNum[(g_deChSelStt-9)/3] = 0;
					ConvertNum2AscHex(g_arSetModMacNum[0], g_arSetModMacAsc, 2);
					ConvertNum2AscHex(g_arSetModMacNum[1], g_arSetModMacAsc+3, 2);
					ConvertNum2AscHex(g_arSetModMacNum[2], g_arSetModMacAsc+6, 2);
					ConvertNum2AscHex(g_arSetModMacNum[3], g_arSetModMacAsc+9, 2);
					ConvertNum2AscHex(g_arSetModMacNum[4], g_arSetModMacAsc+12, 2);
					ConvertNum2AscHex(g_arSetModMacNum[5], g_arSetModMacAsc+15, 2);
					break;
				case 8 :						// Selected DestIp
					if(g_arSetDestIpNum[(g_deChSelStt-9)/3] < g_arSetAddrMax[g_deChSelStt])
						g_arSetDestIpNum[(g_deChSelStt-9)/3] += g_arSetAddrSum[g_deChSelStt];
					else
						g_arSetDestIpNum[(g_deChSelStt-9)/3] = 255;
					ConvertNum2AscDec(g_arSetDestIpNum[0], g_arSetDestIpAsc, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[1], g_arSetDestIpAsc+4, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[2], g_arSetDestIpAsc+8, 3);
					ConvertNum2AscDec(g_arSetDestIpNum[3], g_arSetDestIpAsc+12, 3);
					break;
				case 9 :						// Selected DestPort
					if(g_deSetDestPortNum < g_arSetPortMax[g_deChSelStt])
						g_deSetDestPortNum += g_arSetPortSum[g_deChSelStt];
					else
						g_deSetDestPortNum = 65535;
					ConvertNum2AscDec(g_deSetDestPortNum, g_arSetDestPortAsc, 5);
					break;
				default :
					break;
			}
		}

		g_deSw2ShortFlg = 0;
	}

	if(g_deSw3ShortFlg == 1)		// Change to Details List Select Screen
	{
		if(g_deDetSelStt == 0)		// if Select 'SAVE' Details
		{
			// Save
			memcpy(&CPU_I2C.ModIp, g_arSetModIpNum, 4);
			memcpy(&CPU_I2C.ModSub, g_arSetModSubNum, 4);
			memcpy(&CPU_I2C.ModGw, g_arSetModGwNum, 4);
//			memcpy(&CPU_I2C.ModMac, g_arSetModMacNum, 6);
			memcpy(&CPU_I2C.ModPort, &g_deSetModPortNum, 2);

			memcpy(&ipaddr, &CPU_I2C.ModIp, 4);
			memcpy(&netmask, &CPU_I2C.ModSub, 4);
			memcpy(&gw, &CPU_I2C.ModGw, 4);
			memcpy(&g_deEthModPort, &CPU_I2C.ModPort, 2);

			I2C_WRITE();
			ETH_INIT();
			HAL_Delay(200);

			g_deBkgdStt = SETTING_STATE;
		}
		else						// if Select Other Details
		{
			if(g_deChSelStt == 0)
			{
				g_deChSelStt = 9;
			}
			else if(g_deChSelStt < 25)
			{
				g_deChSelStt++;

				if((g_deDetSelStt == 2) || (g_deDetSelStt == 3) || (g_deDetSelStt == 4) || (g_deDetSelStt == 9)) // if Address
				{
					if((g_deChSelStt == 12) || (g_deChSelStt == 16) || (g_deChSelStt == 20))
						g_deChSelStt++;
					if(g_deChSelStt == 24)
						g_deChSelStt = 0;
				}
//				if(g_deDetSelStt == 5)	// if Mac
//				{
//					if((g_deChSelStt == 11) || (g_deChSelStt == 14) || (g_deChSelStt == 17) || (g_deChSelStt == 20) || (g_deChSelStt == 23))
//						g_deChSelStt++;
//				}
				if((g_deDetSelStt == 5) || (g_deDetSelStt == 10))	// if Port
				{
					if(g_deChSelStt == 14)
						g_deChSelStt = 0;
				}
			}
			else
			{
				g_deChSelStt = 0;
			}
		}
		g_deSw3ShortFlg = 0;
	}
	if(g_deSw4ShortFlg == 1)		// Change to Details List Select Screen
	{
		if(g_deChSelStt == 0)
		{
			g_deBkgdStt = SETTING_STATE;
		}
		else
		{
			g_deChSelStt = 0;
		}

		g_deSw4ShortFlg = 0;
	}
}
void LCD_DetailsSoundStatus(void)
{
	if(g_deSw1ShortFlg == 1)		// Change to Lower Details List Selection
	{
		if(g_deChSelStt == 0)		// if Character is  Unselected,
		{
			if(g_deDetSelStt == 0)	// if Detail(Type, Volume) is Unselected,
				g_deDetSelStt = 2;
			else
				g_deDetSelStt--;
		}
		else						// if Character is Selected
		{
			switch(g_deDetSelStt)
			{
				case 1 :						// Selected Date

					break;
				default :
					break;
			}
		}
		g_deSw1ShortFlg = 0;
	}

	if(g_deSw2ShortFlg == 1)		// Change to Upper Details List Selection
	{
		if(g_deChSelStt == 0)		// if Character is  Unselected,
		{
			if(g_deDetSelStt == 2)	// if Detail(Type, Volume) is Unselected,
				g_deDetSelStt = 0;
			else
				g_deDetSelStt++;
		}
		else						// if Character is Selected
		{
			switch(g_deDetSelStt)
			{
				case 1 :						// Selected Date

					break;
				default :
					break;
			}
		}
		g_deSw2ShortFlg = 0;
	}

	if(g_deSw3ShortFlg == 1)		// Change to Details List Select Screen
	{
		if(g_deDetSelStt == 0)		// if Select 'SAVE' Details
		{
			// Save
			g_deBkgdStt = SETTING_STATE;
		}
		else						// if Select Other Details
		{
			if(g_deChSelStt < 4)
