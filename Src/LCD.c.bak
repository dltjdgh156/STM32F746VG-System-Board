void LCD_MainDisplay(void)
{
	// Background Main Page
	if(g_deBkgdStt == g_deBkgdStt_k)	// After Second Time
	{
		if(g_deMainSelStt != PM10_0_SEL)  // 선택 PM (PM0.5 / PM1.0 / PM2.5 / PM5.0)
		{
			// 나중에 색상 전환에 사용
			g_dePm100Stt_k = WHITE;

