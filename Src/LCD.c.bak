void FUN_backgrounMAIN_STATE_top_text_line(uint8_t u1t_sel_pm)
{
	// 상단 고정 문자
	if(u1t_sel_pm == PM0_5_SEL)
	{
		Ili9341_Ascii(		(int8_t*)"0.5",						// 출력 문자
							u4f_f24_size,				// Font Size = 24
							BLACK,						// Font Color
