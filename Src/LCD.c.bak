			FUN_backgrounMAIN_STATE_top_color(PM_UART.Cnt.Pm005,	u2f_1st_blok, PM0_5_SEL);		// Update Top Color Block(PM0.5)
			FUN_backgrounMAIN_STATE_top_color(PM_UART.Cnt.Pm010,	u2f_2nd_blok, PM1_0_SEL);		// Update Top Color Block(PM1.0)
			FUN_backgrounMAIN_STATE_top_color(PM_UART.Cnt.Pm025,	u2f_3th_blok, PM2_5_SEL);		// Update Top Color Block(PM2.5)
			FUN_backgrounMAIN_STATE_top_color(PM_UART.Cnt.Pm050,	u2f_4th_blok, PM5_0_SEL);		// Update Top Color Block(PM5.0)
		}
		else // 선택 PM : (PM 10)
		{
			// 나중에 색상 전환에 사용
			g_dePm005Stt_k = WHITE;
			g_dePm010Stt_k = WHITE;
			g_dePm025Stt_k = WHITE;
			g_dePm050Stt_k = WHITE;

