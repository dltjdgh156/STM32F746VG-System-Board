//===========================================================================//
//  Function Name       : FUN_backgrounMAIN_STATE_top_color                      //
//  Function Method     : Routine : FUN_background_Routine()                 //
//  Return Value        : void                                               //
//  Arguments           : void                                               //
//  Date                : 2019/06/25                                         //
//  Modify Date         :                                                    //
//  Maker Name          : Lee S.H                                            //
//  Modify Name         :                                                    //
//===========================================================================//
void FUN_backgrounMAIN_STATE_top_color(uint32_t u4t_pm_cnt, uint16_t u2t_sel_block, uint8_t u1t_sel_pm)
{
	//-----------------------------------------------------------------------------
	// 파티클 카운터에 따른 색상 변경
	//-----------------------------------------------------------------------------
	// BLUE
	if(u4t_pm_cnt <= BLUE_CUT)
	{
		u2f_b_text_color = C_BLUE;
	}
	// SKY
	else if(u4t_pm_cnt <= SKY_CUT)
	{
		u2f_b_text_color = C_SKY;
	}
	// GREEN
	else if(u4t_pm_cnt <= GREEN_CUT)
	{
		u2f_b_text_color = C_GREEN;
	}
	// GREENYELLOW
	else if(u4t_pm_cnt <= GREENYELLOW_CUT)
	{
		u2f_b_text_color = C_GREENYELLOW;
	}
	// YELLOW
	else if(u4t_pm_cnt <= YELLOW_CUT)
	{
		u2f_b_text_color = C_YELLOW;
	}
	// ORANGE
	else if(u4t_pm_cnt <= ORANGE_CUT)
	{
		u2f_b_text_color = C_ORANGE;
	}
	// RED
	else
	{
		u2f_b_text_color = C_RED;
	}

	//-----------------------------------------------------------------------------
	// TEXT Background color - State로 저장
	//-----------------------------------------------------------------------------
	if(u1t_sel_pm == PM0_5_SEL)			// PM 0.5인 경우
	{
		// 색상 저장
		g_dePm005Stt = u2f_b_text_color;

		// 상태 변화에만 1회 (Block 색상 변경 - Text 배경 색상 변환)
		if(g_dePm005Stt != g_dePm005Stt_k)
		{
			// 상단 박스 색상 변경
			Ili9341_Block_insert(	g_dePm005Stt,					// 색상
									u2f_W_sc + u2t_sel_block,		// Start X
									u2f_W_sp,						// End X
									u2f_W_ec + u2t_sel_block,		// Start Y
									u2f_W_ep,						// End Y
									0);								// Not USED : 0

			// 상단 고정 문자 + 검정 테두리
			FUN_backgrounMAIN_STATE_top_text_line(u1t_sel_pm);

			// 상태 변화 저장
			g_dePm005Stt_k = g_dePm005Stt;
		}
	}
	else if(u1t_sel_pm == PM1_0_SEL)			// PM 1.0인 경우
	{
		// 색상 저장
		g_dePm010Stt = u2f_b_text_color;

		// 상태 변화에만 1회 (Block 색상 변경 - Text 배경 색상 변환)
